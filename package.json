    "redis",
    "rxjs"
  ],
  "author": "Vsevolod Dolgopolov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zavalit/eventsourcing_rxjs_demo/issues"
  },
  "homepage": "https://github.com/zavalit/eventsourcing_rxjs_demo#readme",
  "dependencies": {
    "@types/node": "^14.0.11",
    "@types/redis": "^2.8.22",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "mongodb": "^3.5.8",
    "node-redis": "^0.1.7",
    "redis": "^3.0.2",
    "rxjs": "^6.5.5"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "typescript": "^3.5.3"
  },
